<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.HomeFragment">

    <TextView
        android:id="@+id/calorieLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Calorie Intake"
        tools:layout_editor_absoluteX="156dp"
        tools:layout_editor_absoluteY="184dp" />

    <TextView
        android:id="@+id/burnedcalorieLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Burned Calories"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="224dp" />

    <TextView
        android:id="@+id/todayLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Todays Stats"
        tools:layout_editor_absoluteX="156dp"
        tools:layout_editor_absoluteY="147dp" />

    <TextView
        android:id="@+id/calorieintakeamount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="amount"
        tools:layout_editor_absoluteX="264dp"
        tools:layout_editor_absoluteY="185dp" />

    <TextView
        android:id="@+id/calorieburnedamount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="amount"
        tools:layout_editor_absoluteX="265dp"
        tools:layout_editor_absoluteY="224dp" />
</androidx.constraintlayout.widget.ConstraintLayout>